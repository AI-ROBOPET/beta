# .env
# Environment variables for AI ROBOPET

# Blockchain settings
BLOCKCHAIN_PROVIDER_URL=https://mainnet.infura.io/v3/your-infura-project-id
NFT_CONTRACT_ADDRESS=0x1234567890abcdef1234567890abcdef12345678
PRIVATE_KEY=your-private-key

# GAME SDK settings
GAME_SDK_BASE_URL=https://api.gamesdk.com/v1
GAME_SDK_API_KEY=your-game-sdk-api-key

# Application settings
APP_ENV=production
APP_DEBUG=false

# Logging settings
LOG_LEVEL=INFO
LOG_FILE=logs/application.log

# Frontend settings
FRONTEND_PUBLIC_URL=https://www.ai-robopet.com
